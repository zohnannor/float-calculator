{
  "name": "float-calculator",
  "version": "2.0.0",
  "private": true,
  "homepage": "http://zohnannor.github.io/float-calculator",
  "description": "With this tool you can convert between decimal representation of numbers and the binary [IEEE 754 standard for floating point numbers](https://en.wikipedia.org/wiki/IEEE_754)",
  "keywords": [
    "float",
    "calculator",
    "binary",
    "double",
    "ieee-754"
  ],
