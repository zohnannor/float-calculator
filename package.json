  "bugs": {
    "url": "https://github.com/Zohnannor/float-calculator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zohnannor/float-calculator.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "parcel serve src/index.html",
    "build": "cross-env NODE_ENV=production parcel build --no-minify src/index.html --public-url .",
    "deploy": "gh-pages -d dist",
    "predeploy": "npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "cross-env": "^7.0.3",
    "gh-pages": "^3.1.0",
    "jest": "^27.3.1",
    "parcel-bundler": "^1.12.5",
    "ts-jest": "^27.0.7",
